{
  "name": "Undercover",
  "version": "0.0.1",
  "description": "Automatically redirect pages to incognito when going to a specific domain.",
  "permissions": ["storage", "windows", "webNavigation"],
  "background": {
    "scripts": ["./build/background.min.js"],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "options.html"
  },
  "manifest_version": 2
}
